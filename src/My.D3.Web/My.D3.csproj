  </ItemGroup>


  <ItemGroup>
    <ProjectReference Include="..\My.D3.Application\My.D3.Application.csproj" />
    <ProjectReference Include="..\My.D3.DataAccess\My.D3.DataAccess.csproj" />
    <ProjectReference Include="..\My.D3.Entity\My.D3.Entity.csproj" />
    <ProjectReference Include="..\My.D3.Util\My.D3.Util.csproj" />
  </ItemGroup>

</Project>
